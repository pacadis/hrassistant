<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="Model.Payslip" table="Payslip">
        <id name="idPayslip" column="idPayslip"/>
        <property name="usernameEmployee" column="usernameEmployee"/>
        <property name="year" column="year"/>
        <property name="month" column="month"/>
        <property name="brutSalary" column="brutSalary"/>
        <property name="netSalary" column="netSalary"/>
        <property name="realizedSalary" column="realizedSalary"/>
        <property name="workedHours" column="workedHours"/>
        <property name="requiredHours" column="requiredHours"/>
    </class>

</hibernate-mapping>
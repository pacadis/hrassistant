<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="Model.Clocking" table="Clocking">
        <id name="usernameEmployee" column="usernameEmployee"/>
        <property name="idRequest" column="idRequest"/>
        <property name="year" column="year"/>
        <property name="month" column="month"/>
        <property name="day" column="day"/>
        <property name="workedHours" column="workedHours"/>
        <property name="requiredHours" column="requiredHours"/>
        <property name="overtimeHours" column="overtimeHours"/>
        <property name="overtimeLeave" column="overtimeLeave"/>
    </class>

</hibernate-mapping>